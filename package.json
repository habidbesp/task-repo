{
  "name": "tractive-task",
  "version": "1.0.0",
  "main": "index.js",
  "homepage": "http://habidbesp.github.io/task-repo",
  "scripts": {
    "watch:sass": "sass src/sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "sass src/sass/main.scss css/style.comp.css",
    "prefix:css": "postcss css/style.comp.css --use autoprefixer -b 'last 10 versions' -o css/style.prefix.css",
    "compress:css": "postcss css/style.prefix.css --output css/style.css",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "build": "npm run build:css && mkdir -p dist && cp -r *.html dist/ && cp -r img dist/ && cp -r css/style.css dist/ && cp -r css/style.css.map dist/",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "autoprefixer": "^10.4.20",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0",
    "sass": "^1.81.0"
  },
  "devDependencies": {
    "cssnano": "^7.0.6",
    "gh-pages": "^6.2.0",
    "live-server": "^1.2.2",
    "postcss": "^8.4.49"
  }
}
